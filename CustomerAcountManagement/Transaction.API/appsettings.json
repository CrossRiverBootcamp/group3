{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error"
      }

    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "TransactionDB",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "TransactionDB": "Server=localhost;database=TransactionDB;Trusted_Connection=True;",
    "NSB": "Server=localhost;database=BankNSB;Trusted_Connection=True;",
    "RabbitMQ": "host=localhost;"

  },
  "AllowedHosts": "*"
}
